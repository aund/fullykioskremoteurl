{

  "description": "Loads the current url in your instance of Fully Kiosk",
  "manifest_version": 2,
  "name": "FullyRemoteURL",
  "version": "1.0",
  "homepage_url": "https://github.com/aund/fullykioskremoteurl",
  "icons": {
    "48": "icons/fully.png"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "https://fullykiosk.i.microfox.co.uk/*",
    "http://192.168.0.212:2323/*"
  ],

  "browser_action": {
    "default_icon": "icons/fully.png",
    "theme_icons": [{
        "light": "icons/fully.png",
        "dark": "icons/fully.png",
        "size": 32
    }],
    "default_title": "Fully"
  },

  "background": {
    "scripts": ["background.js"]
  }

}
